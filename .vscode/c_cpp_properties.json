{
    "env": {
        "z88dk": "${env:Z88DK}"
    },
    "configurations": [
        {
            "name": "z88dk",
            "includePath": [
                "${z88dk}/include/_DEVELOPMENT/clang"
            ],
            "cStandard": "c99",
            "defines": [
                "__INTELLISENSE__",
                "__ZXNEXT",
                "__Z80",
                "__Z80__",
                "__Z80N",
                "__SDCC_IY",
                "__Z88DK_NEWLIB",
                "__Z88DK",
                "__SDCC"   
            ],
            "mergeConfigurations": true
        }
    ],
    "version": 4
}