{
    "env": {
        "z88dk": "${env:Z88DK}"
    },
    "configurations": [
        {
            "name": "z88dk",
            "includePath": [
                "${workspaceFolder}/src",
                "${z88dk}/include/_DEVELOPMENT/clang",
                "${workspaceFolder}/src/engine",
                "${workspaceFolder}/src/audio"
            ],
            "cStandard": "c89",
            "defines": [
                "__INTELLISENSE__",
                "__ZXNEXT",
                "__Z80",
                "__Z80__",
                "__Z80N",
                "__SDCC_IY",
                "__Z88DK_NEWLIB",
                "__Z88DK",
                "__SDCC"   
            ],
            "mergeConfigurations": true,
            "browse": {
                "path": [
                    "${workspaceFolder}/src",
                    "${z88dk}/include"
                ],
                "limitSymbolsToIncludedHeaders": false
            }
        }
    ],
    "version": 4
}